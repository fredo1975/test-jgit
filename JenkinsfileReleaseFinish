pipeline {
    agent any
    tools {
        maven 'Maven 3.3.9'
        jdk 'jdk8'
    }
    stages {
        stage ('Build') {
		 	steps {
		 		sh '''
		 			git remote set-branches --add origin master
					git fetch
            		mvn jgitflow:release-finish -Dmaven.javadoc.skip=true
            	'''
		    }
        }
    }
}
